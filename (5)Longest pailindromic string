-----------------------------------------------------------------------------------------------------------------------
string rev="";
        for(int i=s.size()-1;i>=0;i--) rev=rev+s[i];
        int n=s.size();
        vector<int>pre(n+1,0);
        vector<int>cur(n+1,0);
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n;j++){
                if(s[i-1]==rev[j-1])cur[j]=1+pre[j-1];
                else cur[j]=max(pre[j],cur[j-1]);
            }
            swap(cur,pre);
        }
        return pre[n];
-----------------------------------------------------------------------------------------
